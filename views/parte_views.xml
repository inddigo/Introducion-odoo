<odoo>
    <record id="view_parte_tree" model="ir.ui.view">
        <field name="name">modulo_introduccion.parte.tree</field>
        <field name="model">modulo_introduccion.parte</field>
        <field name="arch" type="xml">
            <list editable="bottom">
                <field name="name"/>
                <field name="producto_id"/>
            </list>
        </field>
    </record>

    <record id="action_partes" model="ir.actions.act_window">
        <field name="name">Partes de Productos</field>
        <field name="res_model">modulo_introduccion.parte</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>